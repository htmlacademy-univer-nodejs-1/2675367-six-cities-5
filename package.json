{
  "name": "six-cities",
  "version": "4.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "start:rest": "npm run build && node ./dist/rest.js",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "cli": "npm run ts src/main.ts --",
    "json-server": "json-server --watch mock/mock-server-data.json --port 3123"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "8.39.0",
    "eslint-config-htmlacademy": "9.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "json-server": "0.17.4",
    "@types/multer": "^1.4.7"
  },
  "dependencies": {
    "axios": "1.7.7",
    "chalk": "5.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "http-status-codes": "^2.2.0",
    "jose": "^4.14.4"
    "multer": "^1.4.5-lts.1",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.0"
  },
  "optionalDependencies": {},
  "peerDependencies": {},
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  }
}
